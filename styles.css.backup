* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

:root {
  /* Green, Red, Earth Tones + Gray Color Palette */
  --primary-green: #2d5a3d;
  --primary-green-light: #3d7a52;
  --primary-green-dark: #1e3d29;
  --accent-red: #c44536;
  --accent-red-dark: #a33327;
  --earth-brown: #8b6f47;
  --earth-tan: #d4a574;
  --earth-beige: #f5ebe0;
  --deep-muted-teal: #2f6f6d;
  --new-color: #c4a685;
  --new-textcolor: #2f2418;
  --new-cardcolor: #f5f1ea;
  --warm-gray: #6b6b6b;
  --light-gray: #f7f7f5;
  --medium-gray: #9a9a9a;
  --dark-gray: #2c2c2c;
  --white: #ffffff;
  --border-color: #e0ddd5;
  --creamish-white: rgb(250, 248, 243);

  /* Semantic Colors */
  --primary-color: var(--primary-green);
  --secondary-color: var(--accent-red);
  --dark-text: var(--dark-gray);
  --medium-text: var(--warm-gray);
  --light-text: var(--medium-gray);
  --light-bg: var(--light-gray);

  /* Spacing */
  --spacing-xs: 0.5rem;
  --spacing-sm: 1rem;
  --spacing-md: 2rem;
  --spacing-lg: 3rem;
  --spacing-xl: 5rem;

  /* Transitions */
  --transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  --transition-fast: all 0.15s ease;
}

html {
  scroll-behavior: smooth;
}

body {
  font-family:
    "Open Sans",
    -apple-system,
    BlinkMacSystemFont,
    "Segoe UI",
    Roboto,
    sans-serif;
  line-height: 1.7;
  color: var(--dark-text);
  background-color: var(--white);
  font-size: 16px;
}

.container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 var(--spacing-md);
}

/* Typography */
h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: "Merriweather", Georgia, serif;
  line-height: 1.3;
  font-weight: 700;
  color: var(--dark-text);
  letter-spacing: -0.01em;
}

h1 {
  font-size: clamp(2.5rem, 5vw, 3.5rem);
}

h2 {
  font-size: clamp(1.75rem, 3vw, 2.25rem);
  margin-bottom: var(--spacing-sm);
}

h3 {
  font-size: 1.25rem;
  margin-bottom: var(--spacing-xs);
}

p {
  color: var(--medium-text);
}

/* Section Styling */
.section {
  padding: var(--spacing-xl) 0;
}

.section-header {
  text-align: center;
  max-width: 600px;
  margin: 0 auto var(--spacing-lg);
}

/* Project Page Sections - Reduced Padding */
.project-overview,
.project-initiatives,
.project-impact,
.project-context,
.project-partners,
.project-cta,
.project-gallery {
  padding: var(--spacing-lg) 0;
}

.section-label {
  font-family: "Open Sans", sans-serif;
  font-size: 0.875rem;
  font-weight: 600;
  color: var(--primary-green);
  text-transform: uppercase;
  letter-spacing: 0.1em;
  margin-bottom: var(--spacing-xs);
}

.section-description {
  color: var(--light-text);
  margin-top: var(--spacing-xs);
}

/* Buttons */
.btn {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  gap: 0.5rem;
  padding: 0.875rem 1.75rem;
  border: none;
  border-radius: 6px;
  font-family: "Open Sans", sans-serif;
  font-size: 0.9375rem;
  font-weight: 600;
  cursor: pointer;
  transition: var(--transition);
  text-decoration: none;
  text-align: center;
}

.btn-primary {
  background-color: var(--primary-green);
  color: var(--white);
}

.btn-primary:hover {
  background-color: var(--primary-green-dark);
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(45, 90, 61, 0.3);
}

.btn-small {
  padding: 0.625rem 1.25rem;
  font-size: 0.875rem;
}

.btn-large {
  padding: 1rem 2rem;
  font-size: 1rem;
}

.btn-full {
  width: 100%;
}

.btn-donate {
  background-color: transparent;
  color: var(--medium-text);
  padding: 0;
  border-radius: 0;
}

.btn-donate:hover {
  background-color: transparent;
  color: var(--primary-green);
}

.btn-volunteer {
  background-color: transparent;
  color: var(--medium-text);
  padding: 0;
  border-radius: 0;
}

.btn-volunteer:hover {
  background-color: transparent;
  color: var(--primary-green);
}

.btn-donate-large {
  background-color: var(--accent-red);
  color: var(--white);
  padding: 0.875rem 1.75rem;
  border-radius: 6px;
  font-weight: 600;
  text-decoration: none;
  display: inline-block;
  transition: var(--transition);
  margin: var(--spacing-sm) 0;
}

.btn-donate-large:hover {
  background-color: var(--accent-red-dark);
  transform: translateY(-2px);
}

.btn-volunteer-large {
  background-color: var(--primary-green);
  color: var(--white);
  padding: 0.875rem 1.75rem;
  border-radius: 6px;
  font-weight: 600;
  text-decoration: none;
  display: inline-block;
  transition: var(--transition);
  margin: var(--spacing-sm) 0;
}

.btn-volunteer-large:hover {
  background-color: var(--primary-green-dark);
  transform: translateY(-2px);
}

/* Navigation */
.navbar {
  background-color: var(--earth-beige);
  border-bottom: 1px solid var(--border-color);
  padding: 0.01rem 0;
  position: sticky;
  top: 0;
  z-index: 1000;
}

.navbar .container {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.nav-logo img {
  display: block;
  height: 100px;
  width: auto;
}

.nav-menu {
  display: flex;
  list-style: none;
  gap: var(--spacing-md);
  align-items: center;
}

.nav-menu a {
  text-decoration: none;
  color: var(--medium-text);
  font-weight: 500;
  font-size: 1.05rem;
  transition: var(--transition-fast);
}

.nav-menu a:hover {
  color: var(--primary-green);
}

/* Dropdown Menu */
.nav-dropdown {
  position: relative;
}

.dropdown-toggle {
  display: flex;
  align-items: center;
  gap: 4px;
}

.dropdown-toggle svg {
  transition: var(--transition-fast);
}

.nav-dropdown:hover .dropdown-toggle svg {
  transform: rotate(180deg);
}

.dropdown-menu {
  position: absolute;
  top: 100%;
  left: 0;
  background: var(--white);
  border: 1px solid var(--border-color);
  border-radius: 8px;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
  min-width: 200px;
  padding: var(--spacing-xs) 0;
  opacity: 0;
  visibility: hidden;
  transform: translateY(10px);
  transition: var(--transition);
  list-style: none;
}

.nav-dropdown:hover .dropdown-menu {
  opacity: 1;
  visibility: visible;
  transform: translateY(0);
}

.dropdown-menu li a {
  display: block;
  padding: 0.75rem 1.25rem;
  color: var(--dark-text);
}

.dropdown-menu li a:hover {
  background-color: var(--light-bg);
  color: var(--primary-green);
}

.mobile-menu-toggle {
  display: none;
  flex-direction: column;
  gap: 5px;
  background: none;
  border: none;
  cursor: pointer;
  padding: 5px;
}

.mobile-menu-toggle span {
  width: 24px;
  height: 2px;
  background-color: var(--dark-text);
  transition: var(--transition);
}

/* Hero Carousel */
.hero-carousel {
  position: relative;
  width: 100%;
  height: 70vh;
  min-height: 500px;
  max-height: 700px;
  overflow: hidden;
}

.carousel-container {
  position: relative;
  width: 100%;
  height: 100%;
}

.carousel-slides {
  position: relative;
  width: 100%;
  height: 100%;
}

.carousel-slide {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  transition: opacity 0.8s ease-in-out;
}

.carousel-slide.active {
  opacity: 1;
}

.carousel-slide img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.carousel-btn {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background: rgba(255, 255, 255, 0.9);
  border: none;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: var(--transition);
  z-index: 10;
  color: var(--dark-text);
}

.carousel-btn:hover {
  background: var(--white);
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.15);
}

.carousel-prev {
  left: 20px;
}

.carousel-next {
  right: 20px;
}

.carousel-dots {
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  gap: 10px;
  z-index: 10;
}

.dot {
  width: 12px;
  height: 12px;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.5);
  border: 2px solid var(--white);
  cursor: pointer;
  transition: var(--transition);
}

.dot.active,
.dot:hover {
  background: var(--white);
}

.carousel-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 5;
  font-family: "Merriweather", Georgia, serif;
  font-size: clamp(2rem, 6vw, 4rem);
  font-weight: 700;
  color: var(--white);
  text-align: center;
  padding: var(--spacing-lg);
  transform: scaleY(2); /* New */
  line-height: 1;
  letter-spacing: 0.02em;
}

/* Mission Section */
.mission-section {
  background-color: var(--earth-beige);
}

.mission-grid {
  display: grid;
  grid-template-columns: 1fr 1.2fr;
  gap: var(--spacing-xl);
  align-items: center;
}

.mission-image img {
  width: 100%;
  border-radius: 12px;
  object-fit: cover;
  aspect-ratio: 4/3;
  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);
}

.mission-content .section-label {
  text-align: left;
}

.mission-content h2 {
  text-align: left;
  margin-bottom: var(--spacing-sm);
  color: var(--primary-green);
}

.mission-tagline {
  font-family: "Merriweather", Georgia, serif;
  font-size: clamp(1.75rem, 3vw, 2.25rem);
  font-weight: 700;
  color: var(--primary-green);
  margin-bottom: var(--spacing-lg);
  line-height: 1.3;
  letter-spacing: -0.01em;
}

.mission-text {
  margin-bottom: var(--spacing-md);
}

.mission-text p {
  margin-bottom: var(--spacing-sm);
  font-size: 1rem;
  line-height: 1.8;
}

/* Impact Section */
.impact-section {
  background-color: var(--new-color);
  padding-top: var(--spacing-lg);
}

.impact-heading {
  text-align: center;
  margin-bottom: var(--spacing-lg);
  color: var(--primary-green);
}

.impact-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: var(--spacing-md);
}

.impact-item {
  text-align: center;
  padding: var(--spacing-md);
  background: var(--new-cardcolor);
  border-radius: 12px;
  transition: var(--transition);
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
}

.impact-item:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
}

.impact-icon {
  width: 64px;
  height: 64px;
  background: var(--primary-green);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto var(--spacing-sm);
  color: var(--white);
}

.impact-item h3 {
  color: var(--primary-green);
  margin-bottom: var(--spacing-xs);
  font-size: 1.125rem;
  font-weight: 700;
}

.impact-item p {
  font-size: 0.9375rem;
  color: var(--dark-text);
  margin: 0;
}

/* Projects Section */
.projects-section {
  background-color: var(--light-bg);
}

.projects-grid {
  display: grid;
  grid-template-columns: repeat(3, 300px);
  gap: var(--spacing-lg);
  justify-content: center;
  padding: 0 2rem;
}

.project-card {
  width: 300px;
  height: 400px;
  perspective: 1000px;
  transition: var(--transition);
}

.project-card-inner {
  position: relative;
  width: 100%;
  height: 100%;
  transition:
    transform 0.6s ease,
    box-shadow 0.3s ease;
  transform-style: preserve-3d;
  border-radius: 12px;
}

.project-card:hover .project-card-inner {
  transform: rotateY(10deg) scale(1.05);
  box-shadow: -20px 20px 50px rgba(0, 0, 0, 0.2);
}

.project-card:hover {
  transform: translateX(-150px);
}

.project-front {
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 12px;
  overflow: hidden;
  backface-visibility: hidden;
  z-index: 10;
}

.project-front img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: var(--transition);
}

.project-title-overlay {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  padding: var(--spacing-md);
  background: linear-gradient(transparent, rgba(0, 0, 0, 0.8));
}

.project-title-overlay h3 {
  color: var(--white);
  margin: 0;
  font-size: 1.25rem;
}

.project-details {
  position: absolute;
  top: 0;
  right: -20px;
  width: 300px;
  height: 100%;
  padding: var(--spacing-md);
  background: var(--white);
  border-radius: 12px;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.15);
  opacity: 0;
  pointer-events: none;
  transition:
    opacity 0.3s ease,
    box-shadow 0.3s ease;
  display: flex;
  flex-direction: column;
  justify-content: center;
  z-index: 20;
}

.project-card:hover .project-details {
  opacity: 1;
  pointer-events: auto;
}

.project-details h3 {
  color: var(--primary-green);
  margin-bottom: var(--spacing-sm);
}

.project-details p {
  font-size: 0.9375rem;
  line-height: 1.6;
  margin-bottom: var(--spacing-sm);
  color: var(--medium-text);
}

.project-details .btn {
  background: var(--primary-green);
  color: var(--white);
  cursor: pointer;
  position: relative;
  z-index: 30;
}

.project-details .btn:hover {
  background: var(--primary-green-dark);
}

/* Take Action Section */
.take-action-section {
  background-color: var(--white);
  padding: var(--spacing-xl) 0;
  padding-top: var(--spacing-lg);
}

.section-header h2 {
  text-align: center;
  color: var(--primary-green);
}

.action-cards-grid {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: var(--spacing-lg);
  max-width: 950px;
  margin: 0 auto;
}

.action-card {
  position: relative;
  overflow: hidden;
  border-radius: 12px;
  aspect-ratio: 1/0.6;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
  transition: var(--transition);
}

.action-card:hover {
  box-shadow: 0 15px 40px rgba(0, 0, 0, 0.15);
  transform: translateY(-4px);
}

.action-card-image {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.action-card-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: var(--transition);
}

.action-card:hover .action-card-image img {
  transform: scale(1.05);
}

.action-card-overlay {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  padding: var(--spacing-lg);
  background: linear-gradient(transparent, rgba(0, 0, 0, 0.7));
  display: flex;
  align-items: flex-end;
  justify-content: center;
  height: 100%;
  transition: var(--transition);
}

.btn-action {
  background-color: var(--accent-red);
  color: var(--white);
  font-size: 1rem;
  font-weight: 600;
  padding: 0.875rem 2rem;
  border-radius: 6px;
  text-decoration: none;
  transition: var(--transition);
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

.btn-action:hover {
  background-color: var(--accent-red-dark);
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(196, 69, 54, 0.3);
}

/* Volunteer Page */
.volunteer-hero {
  position: relative;
  width: 100%;
  height: 500px;
  overflow: hidden;
}

.volunteer-hero img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.volunteer-hero-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  display: flex;
  align-items: center;
  justify-content: center;
}

.volunteer-hero-content {
  text-align: center;
  color: var(--white);
}

.volunteer-hero-content h1 {
  font-size: clamp(2rem, 5vw, 3rem);
  color: var(--white);
  margin-bottom: var(--spacing-sm);
}

.volunteer-hero-content p {
  font-size: 1.25rem;
  color: var(--white);
  margin: 0;
}

.volunteer-section {
  background-color: var(--white);
}

.volunteer-content {
  max-width: 900px;
  margin: 0 auto;
}

.volunteer-heading {
  margin-bottom: var(--spacing-lg);
  text-align: center;
}

.volunteer-heading h2 {
  color: var(--primary-green);
  font-size: clamp(1.75rem, 3vw, 2.25rem);
  margin-bottom: var(--spacing-sm);
}

.volunteer-heading p {
  font-size: 1.125rem;
  line-height: 1.6;
  color: var(--medium-text);
  margin: 0;
}

.volunteer-heading .volunteer-intro-text {
  font-size: 1rem;
  line-height: 1.8;
  color: var(--medium-text);
  margin-top: var(--spacing-md);
  max-width: 100%;
  margin-left: auto;
  margin-right: auto;
}

.volunteer-form {
  display: grid;
  gap: var(--spacing-md);
  background-color: var(--creamish-white);
  border: 1px solid var(--border-color);
  border-radius: 12px;
  padding: var(--spacing-lg);
  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
}

.form-group {
  display: grid;
  grid-template-columns: 200px 1fr;
  gap: var(--spacing-md);
  align-items: center;
}

.form-group label {
  font-family: "Open Sans", sans-serif;
  font-weight: 600;
  color: rgb(51, 51, 51);
  font-size: 0.95rem;
  text-align: left;
}

.form-group .required {
  color: var(--accent-red);
  font-weight: 700;
}

.volunteer-form input,
.volunteer-form select,
.volunteer-form textarea {
  padding: 0.875rem 1rem;
  font-family: "Open Sans", sans-serif;
  font-size: 1rem;
  border: 1px solid var(--border-color);
  border-radius: 6px;
  color: var(--dark-text);
  transition: var(--transition);
}

.volunteer-form input::placeholder,
.volunteer-form textarea::placeholder {
  color: var(--light-text);
}

.volunteer-form input:focus,
.volunteer-form select:focus,
.volunteer-form textarea:focus {
  outline: none;
  border-color: var(--primary-green);
  box-shadow: 0 0 0 3px rgba(45, 90, 61, 0.1);
}

.volunteer-form textarea {
  resize: vertical;
  min-height: 100px;
  font-family: "Open Sans", sans-serif;
}

.form-group.consent-checkbox {
  grid-template-columns: 1fr;
  align-items: flex-start;
  gap: var(--spacing-sm);
}

.form-group.consent-checkbox label {
  display: flex;
  align-items: flex-start;
  gap: var(--spacing-sm);
}

.form-group.consent-checkbox input {
  margin-top: 0.25rem;
  width: auto;
  cursor: pointer;
  accent-color: var(--primary-green);
}

.form-group.consent-checkbox label {
  font-weight: 400;
  font-size: 0.95rem;
  cursor: pointer;
  color: rgb(51, 51, 51);
}

.volunteer-form .btn {
  margin-top: var(--spacing-sm);
  width: 100%;
}

.form-note {
  text-align: center;
  font-size: 0.875rem;
  color: var(--light-text);
  margin-top: var(--spacing-lg);
}

.form-group:has(.checkbox-grid) {
  grid-template-columns: 1fr;
  align-items: flex-start;
}

.form-group:has(.checkbox-grid) > label {
  margin-bottom: var(--spacing-xs);
}

.checkbox-grid {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: var(--spacing-md);
}

.checkbox-item {
  display: flex;
  align-items: center;
  gap: var(--spacing-sm);
}

.checkbox-item input[type="checkbox"] {
  width: 20px;
  height: 20px;
  cursor: pointer;
  accent-color: var(--primary-green);
  flex-shrink: 0;
}

.checkbox-item label {
  cursor: pointer;
  font-weight: 400;
  color: rgb(51, 51, 51);
  margin: 0;
  font-size: 0.95rem;
}

.checkbox-item input[type="checkbox"]:focus {
  outline: 2px solid var(--primary-green);
  outline-offset: 2px;
}

/* Project Page Styles */
.project-hero {
  background-color: var(--primary-green);
  padding: var(--spacing-xl) 0;
  min-height: 300px;
  display: flex;
  align-items: center;
}

.project-hero-overlay {
  width: 100%;
}

.project-hero-content {
  text-align: center;
  color: var(--white);
}

.project-category {
  font-size: 0.875rem;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.1em;
  color: var(--earth-tan);
  margin-bottom: var(--spacing-sm);
}

.project-hero-content h1 {
  font-size: clamp(2rem, 4vw, 2.75rem);
  color: var(--white);
  margin-bottom: var(--spacing-sm);
}

.project-date {
  font-size: 1rem;
  color: rgba(255, 255, 255, 0.8);
  margin: 0;
}

.project-overview {
  background-color: var(--white);
}

.project-content {
  max-width: 800px;
  margin: 0 auto;
}

.project-intro h2 {
  color: var(--primary-green);
  margin-bottom: var(--spacing-md);
  text-align: center;
}

.project-intro p {
  font-size: 1.0625rem;
  line-height: 1.8;
  color: var(--medium-text);
}

.project-context {
  background-color: var(--earth-beige);
}

.context-grid {
  display: grid;
  grid-template-columns: 1fr 400px;
  gap: var(--spacing-xl);
  align-items: center;
}

.context-text h2 {
  color: var(--primary-green);
  margin-bottom: var(--spacing-md);
}

.context-text p {
  font-size: 1rem;
  line-height: 1.8;
  color: var(--medium-text);
  margin-bottom: var(--spacing-md);
}

.context-image,
.initiative-image {
  position: relative;
}

.image-placeholder {
  background-color: var(--light-gray);
  border: 2px dashed var(--border-color);
  border-radius: 8px;
  aspect-ratio: 3/4;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: var(--medium-gray);
  padding: var(--spacing-md);
  text-align: center;
}

.image-placeholder p {
  margin: 0.25rem 0;
  font-size: 1rem;
  font-weight: 600;
}

.placeholder-note {
  font-size: 0.875rem !important;
  font-weight: 400 !important;
  font-style: italic;
}

.video-placeholder {
  background-color: var(--light-gray);
  border: 2px dashed var(--border-color);
  border-radius: 8px;
  aspect-ratio: 3/4;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: var(--medium-gray);
  padding: var(--spacing-md);
  text-align: center;
}

.video-placeholder p {
  margin: 0.25rem 0;
  font-size: 1rem;
  font-weight: 600;
}

.project-initiatives {
  background-color: var(--white);
}

.section-title {
  text-align: center;
  color: var(--primary-green);
  margin-bottom: var(--spacing-lg);
  font-size: clamp(1.75rem, 3vw, 2.25rem);
}

.initiative-item {
  margin-bottom: var(--spacing-xl);
}

.initiative-content {
  display: grid;
  grid-template-columns: 1fr 350px;
  gap: var(--spacing-xl);
  align-items: center;
}

.initiative-item.reverse .initiative-content {
  grid-template-columns: 350px 1fr;
}

.initiative-item.reverse .initiative-text {
  order: 2;
}

.initiative-item.reverse .initiative-image {
  order: 1;
}

.initiative-text h3 {
  color: var(--accent-red);
  margin-bottom: var(--spacing-md);
  font-size: 1.5rem;
}

.initiative-text p {
  font-size: 1rem;
  line-height: 1.8;
  color: var(--medium-text);
  margin-bottom: var(--spacing-md);
}

.project-impact {
  background-color: var(--new-color);
  padding: var(--spacing-xl) 0;
}

.impact-stats {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: var(--spacing-lg);
  margin-bottom: var(--spacing-xl);
  max-width: 800px;
  margin-left: auto;
  margin-right: auto;
}

.stat-card {
  background-color: var(--white);
  padding: var(--spacing-lg);
  border-radius: 12px;
  text-align: center;
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
  transition: var(--transition);
}

.stat-card:hover {
  transform: translateY(-5px);
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.15);
}

.stat-number {
  font-family: "Merriweather", Georgia, serif;
  font-size: 3rem;
  font-weight: 700;
  color: var(--primary-green);
  margin-bottom: var(--spacing-xs);
}

.stat-label {
  font-size: 1rem;
  color: var(--medium-text);
  font-weight: 500;
}

.support-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: var(--spacing-md);
}

.support-item {
  display: flex;
  gap: var(--spacing-sm);
  align-items: flex-start;
  padding: var(--spacing-md);
  background-color: var(--earth-beige);
  border-radius: 8px;
}

.support-icon {
  font-size: 2rem;
  flex-shrink: 0;
}

.support-details strong {
  display: block;
  color: var(--primary-green);
  font-weight: 600;
  margin-bottom: 0.25rem;
}

.support-details p {
  font-size: 0.875rem;
  color: var(--medium-text);
  margin: 0;
}

.project-partners {
  background-color: var(--white);
}

.partners-content {
  max-width: 900px;
  margin: 0 auto;
}

.partners-content > p {
  text-align: center;
  font-size: 1.0625rem;
  color: var(--medium-text);
  margin-bottom: var(--spacing-lg);
}

.partners-list {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: var(--spacing-md);
  margin-bottom: var(--spacing-lg);
}

.partner-item {
  background-color: var(--earth-beige);
  padding: var(--spacing-md);
  border-radius: 8px;
  text-align: center;
}

.partner-item h4 {
  color: var(--primary-green);
  font-size: 1.125rem;
  margin-bottom: var(--spacing-xs);
}

.partner-item p {
  font-size: 0.875rem;
  color: var(--medium-text);
  margin: 0;
}

.volunteer-team {
  background-color: var(--light-bg);
  padding: var(--spacing-lg);
  border-radius: 12px;
  text-align: center;
}

.volunteer-team h4 {
  color: var(--accent-red);
  font-size: 1.25rem;
  margin-bottom: var(--spacing-sm);
}

.volunteer-team p {
  font-size: 1rem;
  line-height: 1.8;
  color: var(--medium-text);
  margin: 0;
}

.project-gallery {
  background-color: var(--earth-beige);
}

.gallery-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: var(--spacing-lg);
  max-width: 1000px;
  margin: 0 auto;
}

.gallery-item {
  position: relative;
}

.project-cta {
  background-color: var(--primary-green);
  text-align: center;
  padding: var(--spacing-xl) 0;
}

.cta-content h2 {
  color: var(--white);
  margin-bottom: var(--spacing-sm);
}

.cta-content p {
  color: rgba(255, 255, 255, 0.9);
  font-size: 1.125rem;
  margin-bottom: var(--spacing-lg);
}

.cta-buttons {
  display: flex;
  gap: var(--spacing-md);
  justify-content: center;
  flex-wrap: wrap;
}

.btn-volunteer-cta {
  background-color: var(--accent-red);
  color: var(--white);
  padding: 0.875rem 1.75rem;
  border-radius: 6px;
  font-weight: 600;
  text-decoration: none;
  display: inline-block;
  transition: var(--transition);
  margin: var(--spacing-sm) 0;
}

.btn-volunteer-cta:hover {
  background-color: var(--accent-red-dark);
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(196, 69, 54, 0.3);
}

/* Project Carousel */
.project-carousel {
  position: relative;
  width: 100%;
  border-radius: 12px;
  overflow: hidden;
  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.15);
  background: var(--light-bg);
}

.carousel-container {
  position: relative;
  width: 100%;
  height: 500px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.carousel-img {
  position: absolute;
  width: 100%;
  height: 100%;
  object-fit: cover;
  opacity: 0;
  transition: opacity 0.6s ease-in-out;
}

.carousel-img.active {
  opacity: 1;
}

.carousel-btn {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  background: rgba(255, 255, 255, 0.8);
  border: none;
  color: var(--dark-gray);
  font-size: 1.5rem;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: var(--transition);
  z-index: 10;
  font-weight: bold;
}

.carousel-btn:hover {
  background: white;
  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.2);
}

.carousel-prev {
  left: 20px;
}

.carousel-next {
  right: 20px;
}

.carousel-indicators {
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  gap: 10px;
  z-index: 10;
}

.indicator {
  width: 12px;
  height: 12px;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.5);
  cursor: pointer;
  transition: var(--transition);
  border: 2px solid transparent;
}

.indicator.active {
  background: white;
  border-color: rgba(0, 0, 0, 0.2);
}

/* Responsive - Project Pages */
@media (max-width: 968px) {
  .context-grid,
  .initiative-content {
    grid-template-columns: 1fr;
  }

  .initiative-item.reverse .initiative-content {
    grid-template-columns: 1fr;
  }

  .initiative-item.reverse .initiative-text {
    order: 1;
  }

  .initiative-item.reverse .initiative-image {
    order: 2;
  }

  .impact-stats {
    grid-template-columns: 1fr;
  }

  .support-grid {
    grid-template-columns: repeat(2, 1fr);
  }

  .partners-list {
    grid-template-columns: 1fr;
  }

  .context-image,
  .initiative-image {
    max-width: 400px;
    margin: 0 auto;
  }

  .gallery-grid {
    grid-template-columns: 1fr;
  }

  .carousel-container {
    height: 350px;
  }

  .carousel-btn {
    width: 40px;
    height: 40px;
    font-size: 1.2rem;
  }

  .carousel-prev {
    left: 10px;
  }

  .carousel-next {
    right: 10px;
  }
}

@media (max-width: 768px) {
  .support-grid {
    grid-template-columns: 1fr;
  }

  .impact-stats {
    grid-template-columns: 1fr;
  }

  .cta-buttons {
    flex-direction: column;
  }

  .cta-buttons .btn {
    width: 100%;
  }
}

/* Lightbox */
.lightbox {
  display: none;
  position: fixed;
  inset: 0;
  background: rgba(0, 0, 0, 0.95);
  z-index: 2000;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  gap: var(--spacing-md);
  padding: var(--spacing-md);
}

.lightbox.active {
  display: flex;
}

.lightbox img {
  max-width: 90%;
  max-height: 80vh;
  object-fit: contain;
  border-radius: 4px;
}

.close-btn {
  position: absolute;
  top: var(--spacing-md);
  right: var(--spacing-md);
  background: none;
  border: none;
  color: white;
  font-size: 2rem;
  cursor: pointer;
  opacity: 0.7;
  transition: var(--transition-fast);
  line-height: 1;
}

.close-btn:hover {
  opacity: 1;
}

.lightbox-nav {
  display: flex;
  gap: var(--spacing-sm);
}

.nav-btn {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  background: rgba(255, 255, 255, 0.1);
  border: 1px solid rgba(255, 255, 255, 0.2);
  color: white;
  padding: 0.75rem 1.25rem;
  border-radius: 6px;
  cursor: pointer;
  transition: var(--transition-fast);
  font-size: 0.875rem;
}

.nav-btn:hover {
  background: rgba(255, 255, 255, 0.2);
}

/* Footer */
.footer {
  background: var(--dark-gray);
  color: var(--white);
  padding: var(--spacing-xl) 0 var(--spacing-md);
}

.footer-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: var(--spacing-lg);
  padding-bottom: var(--spacing-lg);
  border-bottom: 1px solid rgba(255, 255, 255, 0.1);
  margin-bottom: var(--spacing-lg);
}

.footer-section h3 {
  font-family: "Merriweather", Georgia, serif;
  font-size: 1.125rem;
  color: var(--white);
  margin-bottom: var(--spacing-sm);
}

.footer-section p {
  color: rgba(255, 255, 255, 0.7);
  font-size: 0.9375rem;
  line-height: 1.6;
}

.contact-info {
  margin-top: var(--spacing-xs);
  color: var(--earth-tan) !important;
}

.tax-note {
  font-size: 0.8125rem !important;
  color: rgba(255, 255, 255, 0.5) !important;
  margin-top: var(--spacing-sm);
}

.newsletter-form {
  display: flex;
  gap: var(--spacing-xs);
  margin-top: var(--spacing-sm);
}

.newsletter-form input {
  flex: 1;
  padding: 0.75rem 1rem;
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 6px;
  background: rgba(255, 255, 255, 0.1);
  color: var(--white);
  font-size: 0.9375rem;
}

.newsletter-form input::placeholder {
  color: rgba(255, 255, 255, 0.5);
}

.newsletter-form input:focus {
  outline: none;
  border-color: var(--primary-green-light);
  background: rgba(255, 255, 255, 0.15);
}

.newsletter-form .btn {
  padding: 0.75rem 1.25rem;
}

.footer-bottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  gap: var(--spacing-md);
}

.footer-brand {
  display: flex;
  align-items: center;
  gap: var(--spacing-sm);
}

.footer-logo {
  height: 120px;
  width: auto;
}

.footer-brand p {
  color: rgba(255, 255, 255, 0.6);
  font-size: 0.875rem;
  margin: 0;
}

.footer-links {
  display: flex;
  gap: var(--spacing-md);
}

.footer-links a {
  color: rgba(255, 255, 255, 0.7);
  text-decoration: none;
  font-size: 0.9375rem;
  transition: var(--transition-fast);
}

.footer-links a:hover {
  color: var(--white);
}

.footer-copyright {
  text-align: right;
}

.footer-copyright p {
  color: rgba(255, 255, 255, 0.5);
  font-size: 0.875rem;
  margin-bottom: 4px;
}

.footer-copyright .credits {
  font-size: 0.8125rem;
}

/* Responsive Design */
@media (max-width: 1100px) {
  .projects-grid {
    grid-template-columns: repeat(2, 300px);
    gap: var(--spacing-md);
    padding: 0 1rem;
  }

  .project-card:hover {
    transform: translateX(0);
  }

  .project-card:hover .project-details {
    display: none;
  }

  .project-card:hover .project-card-inner {
    transform: scale(1.05);
  }
}

@media (max-width: 968px) {
  .mission-grid {
    grid-template-columns: 1fr;
    gap: var(--spacing-lg);
  }

  .action-cards-grid {
    grid-template-columns: repeat(2, 1fr);
    gap: var(--spacing-md);
  }

  .impact-grid {
    grid-template-columns: repeat(2, 1fr);
  }

  .form-group {
    grid-template-columns: 1fr;
    align-items: flex-start;
  }

  .form-group label {
    margin-bottom: 0.25rem;
  }

  .footer-grid {
    grid-template-columns: repeat(2, 1fr);
  }

  .footer-bottom {
    flex-direction: column;
    text-align: center;
  }

  .footer-copyright {
    text-align: center;
  }
}

@media (max-width: 768px) {
  .navbar .container {
    flex-wrap: wrap;
  }

  .nav-menu {
    display: none;
    width: 100%;
    flex-direction: column;
    gap: 0;
    padding-top: var(--spacing-sm);
    border-top: 1px solid var(--border-color);
    margin-top: var(--spacing-sm);
  }

  .nav-menu.active {
    display: flex;
  }

  .nav-menu li {
    width: 100%;
  }

  .nav-menu a {
    display: block;
    padding: var(--spacing-sm) 0;
    text-align: center;
  }

  .nav-dropdown:hover .dropdown-menu {
    position: static;
    opacity: 1;
    visibility: visible;
    transform: none;
    box-shadow: none;
    border: none;
    background: var(--light-bg);
  }

  .mobile-menu-toggle {
    display: flex;
  }

  .hero-carousel {
    height: 50vh;
    min-height: 350px;
  }

  .carousel-btn {
    width: 40px;
    height: 40px;
  }

  .volunteer-hero {
    height: 350px;
  }

  .checkbox-grid {
    grid-template-columns: 1fr;
  }

  .footer-grid {
    grid-template-columns: 1fr;
    gap: var(--spacing-md);
  }

  .footer-links {
    flex-wrap: wrap;
    justify-content: center;
  }
}

@media (max-width: 768px) {
  .action-cards-grid {
    grid-template-columns: 1fr;
  }
}

@media (max-width: 480px) {
  .section {
    padding: var(--spacing-lg) 0;
  }

  .container {
    padding: 0 var(--spacing-sm);
  }

  .projects-grid {
    grid-template-columns: 1fr;
    gap: var(--spacing-md);
    padding: 0;
  }

  .impact-grid {
    grid-template-columns: 1fr;
  }

  .action-cards-grid {
    grid-template-columns: 1fr;
    gap: var(--spacing-md);
  }

  .action-card {
    aspect-ratio: 1/0.7;
  }

  .project-card {
    width: 100%;
    max-width: 300px;
  }

  .newsletter-form {
    flex-direction: column;
  }

  .carousel-prev {
    left: 10px;
  }

  .carousel-next {
    right: 10px;
  }
}
